{
  "name": "didesktop",
  "productName": "Didesktop",
  "version": "1.0.3",
  "description": "Dental Intel Desktop",
  "license": "MIT",
  "repository": "mporter-di/didesktop",
  "author": {
    "name": "Matt Porter",
    "email": "mporter@dentalintel.com",
    "url": "dentalintel.com"
  },
  "scripts": {
    "test": "xo",
    "start": "electron . --disable-gpu",
    "build": "electron-packager . --out=dist --asar --overwrite --platform=win32 --arch=ia32",
    "pack": "build --dir",
    "dist": "build --ia32",
    "publish": "build -p always",
    "postinstall": "install-app-deps"
  },
  "build": {
    "win": {
      "target": "NSIS"
    }
  },
  "files": [
    "index.js",
    "index.html",
    "timeFrame.html",
    "toaster.html",
    "index.css"
  ],
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "auto-updater": "^1.0.2",
    "electron-debug": "^1.0.0",
    "electron-log": "^2.2.6",
    "electron-packager": "^8.7.2",
    "electron-updater": "^2.2.1"
  },
  "devDependencies": {
    "devtron": "^1.1.0",
    "electron": "^1.3.3",
    "electron-builder": "^18.6.2",
    "electron-packager": "^8.0.0",
    "auto-updater": "^1.0.2",
    "xo": "^0.16.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
